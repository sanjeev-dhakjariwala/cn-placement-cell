<link rel="stylesheet" href="/css/signIn.css" />
<nav class="navbar">
  <a class="navbar-brand" href="#">Placement Cell</a>
  <div class="nav-items">
    <a class="nav-item" href="/signUp">SignUp</a>
  </div>
</nav>
<section>
  <h1>Sign In To Your Account</h1>
  <form action="/sign_in" method="POST">
    <input
      id="email"
      type="text"
      name="email"
      placeholder="Enter Email"
    /><br /><br />
    <input
      id="password"
      type="password"
      name="password"
      placeholder="Enter Password"
    /><br /><br />
    <button id="button">SignIn</button>
  </form>
</section>
